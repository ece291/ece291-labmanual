<!--
     Creates entities for each chapter in the lab manual.  Each entity is named
     chap.foo, where foo is the value of the id attribute on that chapter, and
     corresponds to the name of the directory in which that chapter's .sgml
     file is stored.

     Chapters should be listed in the order in which they are referenced.

     $Id: chapters.ent,v 1.9 2001/09/14 06:48:33 pete Exp $
-->

<!ENTITY chap.preface               SYSTEM "preface.sgml">
<!ENTITY chap.introduction          SYSTEM "introduction/chapter.sgml">
<!ENTITY chap.using-pc              SYSTEM "using-pc/chapter.sgml">
<!ENTITY chap.text-editors          SYSTEM "text-editors/chapter.sgml">
<!ENTITY chap.debugging             SYSTEM "debugging/chapter.sgml">
<!ENTITY chap.assembly              SYSTEM "assembly/chapter.sgml">
<!ENTITY chap.nasm                  SYSTEM "nasm/chapter.sgml">
<!ENTITY chap.c-prog                SYSTEM "c-prog/chapter.sgml">
<!ENTITY chap.libraries             SYSTEM "libraries/chapter.sgml">
<!ENTITY chap.data-structures       SYSTEM "data-structures/chapter.sgml">
<!ENTITY chap.io-devices            SYSTEM "io-devices/chapter.sgml">
<!ENTITY chap.graphics              SYSTEM "graphics/chapter.sgml">
<!ENTITY chap.serial-comm           SYSTEM "serial-comm/chapter.sgml">
<!ENTITY chap.parallel-comm         SYSTEM "parallel-comm/chapter.sgml">

<!-- Appendices -->
<!ENTITY chap.ascii-code            SYSTEM "ascii-code/appendix.sgml">
<!ENTITY chap.inst-ref              SYSTEM "inst-ref/appendix.sgml">

<!-- Glossary and Index -->
<!ENTITY chap.glossary              SYSTEM "glossary.sgml">
